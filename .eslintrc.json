{
  "env": {
      "jest": true,
      "browser": true,
      "es6": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
      "allowTernary": true,
      "no-plusplus": "off",
      "no-console": "off",
      "no-alert": "off",
      "import/extensions": ["error", "ignorePackages"],
      "max-len": ["error", { "code": 500 }]
  }
}
